{
    "name": "sil2/vidflex",
    "description": "FidFlex API Test",
    "keywords": ["laravel"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Oleg Stremin"
        }
    ],
    "require": {
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.11",
        "efureev/laravel-trees": "^3.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Sil2\\Vidflex\\": "src/",
            "Sil2\\Vidflex\\Database\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sil2\\Vidflex\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sil2\\Vidflex\\VidflexServiceProvider"
            ]
        }
    }
}
